  "schema": {
    "schemaName": "snowflake.staging_customer_data",
    "platform": "urn:li:dataPlatform:snowflake",
    "fields": [
      {
        "fieldPath": "customer_id",
        "type": {"type": "VARCHAR"},
        "description": "Cleaned and validated customer identifier",
        "nullable": false
      },
      {
        "fieldPath": "full_name",
        "type": {"type": "VARCHAR"},
        "description": "Concatenated full name from first and last name",
        "nullable": true
      },
      {
        "fieldPath": "email_hash",
        "type": {"type": "VARCHAR"},
        "description": "Hashed email address for privacy protection",
        "nullable": true
      },
      {
        "fieldPath": "phone_clean",
        "type": {"type": "VARCHAR"},
        "description": "Cleaned and formatted phone number",
        "nullable": true
      },
      {
        "fieldPath": "registration_year",
        "type": {"type": "INTEGER"},
        "description": "Extracted year from registration date",
        "nullable": true
      }
    ]
  },
  "profile": {
    "rowCount": 4800,
